{
    "name": "ma-residence/php-sdk",
    "type": "library",
    "require": {
        "php": "8.*",
        "guzzlehttp/guzzle": "7.5.*",
        "symfony/cache": "4.4.*",
        "predis/predis": "2.*",
        "snc/redis-bundle": "4.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*"
    },
    "autoload": {
        "psr-4": {
            "MR\\SDK\\": "src/php-sdk"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MR\\SDK\\": "tests/php-sdk"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    }
}
