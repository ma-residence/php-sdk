{
    "name": "ma-residence/php-sdk",
    "type": "library",
    "require": {
        "php": "8.*",
        "guzzlehttp/guzzle": "6.*",
        "symfony/cache": "4.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "MR\\SDK\\": "src/php-sdk"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MR\\SDK\\": "tests/php-sdk"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "master-dev"
        }
    }
}
