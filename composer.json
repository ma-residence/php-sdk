{
    "name": "ma-residence/php-sdk",
    "type": "library",
    "require": {
        "php": "^5.6 || ^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/cache": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "MR\\SDK\\": "src/php-sdk"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MR\\SDK\\": "tests/php-sdk"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "master-dev"
        }
    }
}
