{
    "name": "ma-residence/php-sdk",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/cache": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "friendsofphp/php-cs-fixer": "2.14.1"
    },
    "autoload": {
        "psr-4": {
            "MR\\SDK\\": "src/php-sdk"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MR\\SDK\\": "tests/php-sdk"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "master-dev"
        }
    }
}
