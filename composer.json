{
    "name": "ma-residence/php-sdk",
    "type": "library",
    "require": {
        "php": "8.*",
        "guzzlehttp/guzzle": "7.5.*",
        "symfony/cache": "5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*",
        "symfony/browser-kit": "6.2.*",
        "symfony/dom-crawler": "6.2.*",
        "symfony/var-dumper": "6.2.*",
        "symfony/var-exporter": "6.2.*",
        "symfony/dotenv": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "MR\\SDK\\": "src/php-sdk"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MR\\SDK\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    }
}
